# WalletConnect Project ID (Required for RainbowKit)
# Get your project ID from: https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Enable Testnets (Optional)
# Set to 'true' to enable testnets like Sepolia
NEXT_PUBLIC_ENABLE_TESTNETS=false

# Alchemy API Key (Optional - for better RPC performance)
# Get your API key from: https://www.alchemy.com/
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here

# Infura API Key (Optional - alternative RPC provider)
# Get your API key from: https://infura.io/
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key_here

# Etherscan API Key (Optional - for contract verification)
# Get your API key from: https://etherscan.io/apis
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# BSCScan API Key (Optional)
BSCSCAN_API_KEY=your_bscscan_api_key_here

# CoinGecko API Key (Optional - for price data)
# Free tier available at: https://www.coingecko.com/en/api
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_api_key_here

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=your_google_analytics_id
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token

# Environment
NODE_ENV=development